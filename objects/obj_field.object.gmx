    <event eventtype="3" enumb="0">
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>
var mx = mouse_x;
var my = mouse_y;
var abs_x = mx - draw_x;
var abs_y = my - draw_y;
mouse_prev_pos_x = mouse_pos_x;
mouse_prev_pos_y = mouse_pos_y;
mouse_pos_x = -1;
mouse_pos_y = -1;
if (abs_x &gt;= 0 &amp;&amp; abs_y &gt;= 0) {
    var x1 = (abs_x - cell_size + cell_xstep) div cell_xstep -1;
    var y1 = abs_y div cell_h;
    var y2 = (abs_y + cell_half_h) div cell_h - 1;
    if (abs(x1) %2) {
        var swap = y1;
        y1 = y2;
        y2 = swap;
    }
    var list_x = ds_list_create();
    var list_y = ds_list_create();
    ds_list_add(list_x, x1);
    ds_list_add(list_y, y1);
    ds_list_add(list_x, x1 + 1);
    ds_list_add(list_y, y2);
    if (ds_list_size(list_x) &gt; 0) {
        var x_min = ds_list_find_value(list_x, 0);
        var y_min = ds_list_find_value(list_y, 0);
        c_x1 = 1.5 * cell_size * x_min + cell_size;
        c_y1 = 2 * y_min * cell_half_h + ((abs(x_min) %2) * cell_half_h) + cell_half_h
        var min_dist = point_distance(c_x1, c_y1, abs_x, abs_y);
        var new_x = ds_list_find_value(list_x, 1);
        var new_y = ds_list_find_value(list_y, 1);
        c_x2 = 1.5 * cell_size * new_x + cell_size;
        c_y2 = 2 * new_y * cell_half_h + ((abs(new_x) %2) * cell_half_h) + cell_half_h
        var new_dist = point_distance(c_x2, c_y2, abs_x, abs_y);
        if (new_dist &lt; min_dist) {
            min_dist = new_dist;
            x_min = new_x;
            y_min = new_y;
        }
        mouse_pos_x = x_min;
        mouse_pos_y = y_min;
    }
    ds_list_destroy(list_x);
    ds_list_destroy(list_y);
    if (scr_in(mouse_pos_x, 0, map_w) &amp;&amp; scr_in(mouse_pos_y, 0, map_h)) {
        if (mouse_check_button_pressed(mb_left) || (mouse_check_button(mb_left) &amp;&amp; (mouse_prev_pos_x != mouse_pos_x || mouse_prev_pos_y != mouse_pos_y))) {
            ds_grid_set(map, mouse_pos_x, mouse_pos_y, 1);
            scr_draw_field();
        }
        if (mouse_check_button_pressed(mb_right) || (mouse_check_button(mb_right) &amp;&amp; (mouse_prev_pos_x != mouse_pos_x || mouse_prev_pos_y != mouse_pos_y))) {
            ds_grid_set(map, mouse_pos_x, mouse_pos_y, 0);
            scr_draw_field();
        }
    }
}

</string>
          </argument>
        </arguments>
      </action>
